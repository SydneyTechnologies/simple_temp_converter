{
  "openapi": "3.1.0",
  "info": { "title": "Temperature Converter", "version": "0.1.0" },

  "servers": [{ "url": "https://simpletempconverter.up.railway.app" }],
  "paths": {
    "/temperature": {
      "get": {
        "summary": "Read Temperature",
        "operationId": "ReadTemperatureType",
        "parameters": [
          {
            "name": "input",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Input"
            }
          }
        ]
      }
    },
    "/tofahrenheit": {
      "get": {
        "summary": "Convert To Fahrenheit",
        "operationId": "CelsiusToFahrenheit",
        "parameters": [
          {
            "name": "value",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Value"
            }
          }
        ]
      }
    },
    "/tocelsius": {
      "get": {
        "summary": "Convert To Celsius",
        "operationId": "FahrenheitToCelsius",
        "parameters": [
          {
            "name": "value",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Value"
            }
          }
        ]
      }
    },
    "/image": {
      "get": {
        "summary": "Get Image Representing the Input Temperature",
        "operationId": "GetTemperatureImage",
        "parameters": [
          {
            "name": "value",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Value"
            }
          },
          {
            "name": "temp_type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Temp Type"
            }
          }
        ]
      }
    },

    "/get_report": {
      "get": {
        "summary": "Get Temperature Report",
        "operationId": "GetTemperatureReport",
        "parameters": [
          {
            "name": "joke",
            "description": "A temperature joke that is relevant to the given temperature",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Joke"
            }
          },
          {
            "name": "image",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Image"
            }
          },
          {
            "name": "celsius",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Celsius"
            }
          },
          {
            "name": "fahrenheit",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number",
              "title": "Fahrenheit"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },

    "/test": {
      "post": {
        "summary": "Test",
        "operationId": "test_test_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/WsbData" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Report": {
        "properties": {
          "joke": {
            "type": "string",
            "title": "Joke"
          },
          "image": {
            "type": "string",
            "title": "Image"
          },
          "celsius": {
            "type": "number",
            "title": "Celsius"
          },
          "fahrenheit": {
            "type": "number",
            "title": "Fahrenheit"
          }
        },
        "type": "object",
        "required": ["joke", "image", "celsius", "fahrenheit"],
        "title": "Report"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      },
      "WsbData": {
        "properties": {
          "websiteTitle": {
            "type": "string",
            "title": "Websitetitle",
            "default": "Mandy's Website"
          },
          "country": { "type": "string", "title": "Country", "default": "" },
          "city": { "type": "string", "title": "City", "default": "" },
          "gmbKey": { "type": "string", "title": "Gmbkey" },
          "group": { "type": "boolean", "title": "Group", "default": true },
          "concept": { "type": "string", "title": "Concept" },
          "aboutKeywords": { "type": "string", "title": "Aboutkeywords" },
          "optionSet": {
            "items": {},
            "type": "array",
            "title": "Optionset",
            "default": []
          },
          "submitCount": {
            "type": "integer",
            "title": "Submitcount",
            "default": 1
          },
          "defaultColor": {
            "type": "object",
            "title": "Defaultcolor",
            "default": {
              "accentColor": [
                "HSL",
                0.01486199575371549,
                0.6460905349794239,
                0.5235294117647059,
                1
              ],
              "mainColor": ["HSL", 0, 0, 0.9725490196078431, 1],
              "whiteColor": ["HSL", 0, 0, 1, 1],
              "blackColor": ["HSL", 0.5645, 0, 0.23529411764705882, 1]
            }
          },
          "addressData": {
            "type": "object",
            "title": "Addressdata",
            "default": {}
          },
          "language": {
            "type": "string",
            "title": "Language",
            "default": "en_gb"
          },
          "additionalData": { "type": "object", "title": "Additionaldata" },
          "addShopSection": {
            "type": "boolean",
            "title": "Addshopsection",
            "default": false
          }
        },
        "type": "object",
        "required": ["gmbKey", "concept", "aboutKeywords", "additionalData"],
        "title": "WsbData"
      }
    }
  }
}
